{
  "expo": {
    "name": "Kula",
    "slug": "kula",
    "version": "0.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1a1a2e"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kula.app",
      "icon": "./assets/icon.png",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#1a1a2e"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Kula использует вашу геолокацию для отображения событий рядом с вами на карте.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Kula использует вашу геолокацию для отображения событий рядом с вами на карте."
      }
    },
    "android": {
      "icon": "./assets/icon.png",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#1a1a2e"
      },
      "package": "com.kula.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Kula использует вашу геолокацию для отображения событий рядом с вами на карте."
        }
      ]
    ]
  }
}